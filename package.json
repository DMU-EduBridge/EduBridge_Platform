{
  "name": "edubridge-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "erd:generate": "prisma generate && echo 'ERD generated at docs/erd.svg'",
    "db:deploy": "prisma migrate deploy",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf .next node_modules/.cache",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "db:cleanup-orphans": "tsx scripts/cleanup-orphan-attempts.ts"
  },
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.87.0",
    "@tanstack/react-query-devtools": "^5.87.0",
    "axios": "^1.11.0",
    "chromadb": "^3.0.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.5",
    "openai": "^5.22.0",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^10.1.8",
    "node-loader": "^2.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^6.15.0",
    "prisma-erd-generator": "^2.1.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3",
    "web-vitals": "^5.1.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
